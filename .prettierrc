{
  "tabWidth": 2,
  "printWidth": 80,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "ignore",
  "proseWrap": "never",
  "parser": "babel-ts",
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "useTabs": false,
  "bracketSameLine": true,
  "bracketSpacing": true
}
